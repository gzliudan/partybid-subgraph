{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "eslint-config-airbnb", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
	"sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
    },
    "plugins": ["prettier", "@typescript-eslint"]
}
